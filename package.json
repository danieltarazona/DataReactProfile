{
  "name": "data-next-gen-profile",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite build --mode client && wrangler dev --remote",
    "dev:local": "vite build --mode client && wrangler dev",
    "build": "vite build --mode client && vite build",
    "preview": "wrangler dev --remote",
    "upload": "git add . && git commit -m 'DataReactProfile Refactor' && git push origin main",
    "sync:core": "CLOUDFLARE_ACCESS_CLIENT_ID='pass://DATA/Cloudflare_Access/client_id' CLOUDFLARE_ACCESS_CLIENT_SECRET='pass://DATA/Cloudflare_Access/client_secret' pass-cli run -- deno run --allow-all ../scripts/sync-library.ts @datakit/react-core",
    "sync:login": "CLOUDFLARE_ACCESS_CLIENT_ID='pass://DATA/Cloudflare_Access/client_id' CLOUDFLARE_ACCESS_CLIENT_SECRET='pass://DATA/Cloudflare_Access/client_secret' pass-cli run -- deno run --allow-all ../scripts/sync-library.ts @datakit/cloudflare-login",
    "sync:latest": "pnpm run sync:core && pnpm run sync:login"
  },
  "dependencies": {
    "@datakit/cloudflare-login": "file:libs/datakit-cloudflare-login-latest.tgz",
    "@datakit/react-core": "file:libs/datakit-react-core-latest.tgz",
    "@uiw/react-md-editor": "^4.0.11",
    "@xstate/react": "^6.0.0",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.45.1",
    "hono": "^4.12.3",
    "i18next": "^25.7.4",
    "i18next-browser-languagedetector": "^8.2.0",
    "lucide-react": "^0.446.0",
    "motion": "^12.26.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^16.5.3",
    "react-router-dom": "^6.26.2",
    "xstate": "^5.25.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20260305.0",
    "@hono/vite-build": "^1.9.3",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.1",
    "postcss": "^8.4.38",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.5.3",
    "vite": "^5.4.1",
    "wrangler": "^4.69.0"
  },
  "pnpm": {
    "overrides": {
      "@react-pdf/renderer": "4.3.2"
    }
  }
}