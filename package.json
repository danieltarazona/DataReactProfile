{
  "name": "data-next-gen-profile",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"wrangler pages dev --port 8788\" \"next dev --turbopack\"",
    "dev:local": "concurrently \"wrangler pages dev --port 8788\" \"next dev --turbopack\"",
    "dev:next": "next dev --turbopack",
    "build": "next build --webpack",
    "start": "next start",
    "lint": "eslint",
    "typecheck": "tsc --noEmit",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "preview": "pnpm build && wrangler pages dev out",
    "deploy": "./scripts/deploy.sh",
    "upload": "git add . && git commit -m 'DataReactProfile' && git push origin main",
    "publish": "git checkout dev && git add . && git commit -m 'DataKitReactProfile' && git push && git checkout main && git merge dev && git push",
    "sync:core": "CLOUDFLARE_ACCESS_CLIENT_ID='pass://DATA/Cloudflare_Access/client_id' CLOUDFLARE_ACCESS_CLIENT_SECRET='pass://DATA/Cloudflare_Access/client_secret' pass-cli run -- deno run --allow-all ../scripts/sync-library.ts @datakit/react-core",
    "sync:login": "CLOUDFLARE_ACCESS_CLIENT_ID='pass://DATA/Cloudflare_Access/client_id' CLOUDFLARE_ACCESS_CLIENT_SECRET='pass://DATA/Cloudflare_Access/client_secret' pass-cli run -- deno run --allow-all ../scripts/sync-library.ts @datakit/cloudflare-login",
    "sync:latest": "pnpm run sync:core && pnpm run sync:login"
  },
  "dependencies": {
    "@datakit/cloudflare-login": "file:libs/datakit-cloudflare-login-latest.tgz",
    "@datakit/react-core": "file:libs/datakit-react-core-latest.tgz",
    "@emotion/is-prop-valid": "^1.4.0",
    "@uiw/react-md-editor": "^4.0.11",
    "@xstate/react": "^6.0.0",
    "clsx": "^2.1.1",
    "hono": "^4.4.7",
    "i18next": "^25.7.4",
    "i18next-browser-languagedetector": "^8.2.0",
    "motion": "^12.26.2",
    "next": "16.1.1",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-i18next": "^16.5.3",
    "xstate": "^5.25.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20260116.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^20.19.30",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "eslint": "^9.39.2",
    "eslint-config-next": "16.1.1",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vitest": "^2.0.0",
    "concurrently": "^9.1.2",
    "wrangler": "^4.64.0"
  },
  "packageManager": "pnpm@10.29.3",
  "pnpm": {
    "overrides": {
      "@react-pdf/renderer": "4.3.2"
    }
  }
}